+						$this->gym_modules[$mode][$module] = $module;
+					}
+				}
+			}
+			@closedir($dir);
+			// Reorder a bit, put the main panel at the first position, others will keep
+			// the file system sorting
+			if (!empty($this->gym_modules[$mode]['main'])) {
+				$main = $this->gym_modules[$mode]['main'];
