+function obtain_gym_config($mode, &$cfg_array) {
+	global $db, $cache;
+	$sql_config = '';
+	$cache_file = '_gym_config';
+	$mode = empty($mode) ? 'main' : $mode;
+	if ($mode != 'main') {
+		$sql_config = 	"WHERE config_type = 'main' OR config_type = '" . $db->sql_escape($mode). "'";
