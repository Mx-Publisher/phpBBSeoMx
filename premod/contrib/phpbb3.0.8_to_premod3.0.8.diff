+												}
+											}
+										}
+										if (!isset($this->old_config[$type_module . '_installed'])) {
+											// Set the module as installed
+											set_gym_config($type_module . '_installed', 1, 'main', $this->old_config);
+											$this->config_report[] = "ACTIVATED <b>$type_module module</b>";
