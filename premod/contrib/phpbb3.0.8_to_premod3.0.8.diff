+		$sql = "SELECT *
+			FROM " . GYM_CONFIG_TABLE . "
+			$sql_config";
+		$db->sql_return_on_error(true);
+		$result = $db->sql_query($sql);
+		$db->sql_return_on_error(false);
+		while ($row = $db->sql_fetchrow($result)) {
+			$cfg_array[$row['config_name']] = $row['config_value'];
