+		for (var i = 0; i < elements.length; i++) {
+			var el = elements[i];
+			if (el.className == 'html') {
+				el.innerHTML = el.firstChild.data;
+			}
+		}
+	} 
+	for (i = 0; i <= onload_functions.length; i++) {
+		eval(onload_functions[i]);
+	}
+}
+window.onunload = function() {
