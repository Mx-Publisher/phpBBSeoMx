+		$dir = opendir( $cache->cache_dir );
+		$action_from_file = '';
+		while( ($file = @readdir($dir)) !== false ) {
+			if(preg_match('`^data_gym_' . $option . '[a-z0-9_-]+\.' . $phpEx . '$`i', $file)) {
