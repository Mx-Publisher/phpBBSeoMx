+			$seo_advice .= '<li>&nbsp;' . $user->lang['SEO_ADVICE_WORDS'] . '</li>';
+		}
+		return $seo_advice ? '<ul  style="color:red">' . $seo_advice . '</ul>' : '';
+	}
