+	<xsl:choose>
+		<xsl:when test="contains($input, '{ROOT_URL}')"><xsl:value-of select="substring-after($input, '{ROOT_URL}')"/></xsl:when>
+		<xsl:otherwise><xsl:value-of select="substring-after($input, '{HTTP_PROTO_REQUEST}')"/></xsl:otherwise>
+	</xsl:choose>
+</xsl:template>
+</xsl:stylesheet>
