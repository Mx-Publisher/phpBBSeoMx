+		// Load the required stylsheet template
+		if ( $load_phpbb_css && $gym_style_type == 'css' ) {
+			@ini_set('user_agent','GYM Sitemaps &amp; RSS / www.phpBB-SEO.com');
+			@ini_set('default_socket_timeout', 10);
