+	color: #666666;
+}
+fieldset.polls dl.voted {color: #000000;
+}
+fieldset.polls dd div {color: #FFFFFF;
+}
+.rtl .pollbar1, .rtl .pollbar2, .rtl .pollbar3, .rtl .pollbar4, .rtl .pollbar5 {border-right-color: transparent;
