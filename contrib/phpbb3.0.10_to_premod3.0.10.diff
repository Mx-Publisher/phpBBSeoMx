+class seo_meta {
+	var $version = '0.4.6';
+	var $meta = array('title' => '', 'description' => '', 'keywords' => '', 'lang' => '', 'category' => '', 'robots' => '', 'distribution' => '', 'resource-type' => '', 'copyright' => '');
+	var $meta_def = array();
+	var $filters = array('description' => 'meta_filter_txt', 'keywords' => 'make_keywords');
